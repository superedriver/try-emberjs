<h1>Books</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Title</th>
        <th>Author</th>
    </tr>
    </thead>
    <tbody>
    {{#each model as |book|}}
        <tr>
            <td>{{book.title}}</td>
            <td>{{book.author.name}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
